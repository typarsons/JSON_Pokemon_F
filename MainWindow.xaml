<Window x:Class="JSON_Pokemon_F.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JSON_Pokemon_F"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="lstPokemon" HorizontalAlignment="Left" Height="391" Margin="25,22,0,0" VerticalAlignment="Top" Width="334" SelectionChanged="lstPokemon_SelectionChanged"/>
        <Image x:Name="imgPokemon" HorizontalAlignment="Left" Height="255" Margin="395,158,0,0" VerticalAlignment="Top" Width="371"/>
        <Label x:Name="lbName" Content="Label" HorizontalAlignment="Left" Height="36" Margin="412,22,0,0" VerticalAlignment="Top" Width="355"/>
        <Label x:Name="lbHeight" Content="Label" HorizontalAlignment="Left" Height="36" Margin="412,63,0,0" VerticalAlignment="Top" Width="147"/>
        <Label x:Name="lbWeight" Content="Label" HorizontalAlignment="Left" Height="36" Margin="618,63,0,0" VerticalAlignment="Top" Width="150"/>
        <ListBox x:Name="lstSprite" Margin="658,350,0,0"/>

    </Grid>
</Window>
